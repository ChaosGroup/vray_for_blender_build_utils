+        else {
+            for(p = 0; p < psys->totpart; ++p) {
+                for(s = 0; s < interp_points_count; ++s) {
+                    WRITE_HEX_VECTOR(gfile, color);
+                }
+            }
+        }
+        fprintf(gfile,"\")));");
+
+
+        fprintf(gfile, "\n\topacity=1.0;");
+        if(psys->part->flag & PART_HAIR_BSPLINE) {
+            fprintf(gfile, "\n\tgeom_splines=1;");
