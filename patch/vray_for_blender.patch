+#if USE_DEBUG == 0
+#  define DEBUG_PRINT(use_debug, ...)
+#else
+#  define DEBUG_PRINT(use_debug, ...) \
+    if(use_debug) { \
+        fprintf(stdout, OUTPUT_PROMPT); \
+        fprintf(stdout, __VA_ARGS__); \
+        fprintf(stdout, "\n"); \
+    }
