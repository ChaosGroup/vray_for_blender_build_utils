+if env['OURPLATFORM'] in ('win32-vc', 'win32-mingw', 'linuxcross', 'win64-vc'):
+	incs += ' ' + env['BF_PTHREADS_INC']
+
+env.BlenderLib('vb_exporter', sources, Split(incs), [], libtype=['core'], priority=[300])
