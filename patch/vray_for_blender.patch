+#  define COLOR_YELLOW   "\033[0;33m"
+#  define COLOR_BLUE     "\033[0;34m"
+#  define COLOR_MAGENTA  "\033[0;35m"
+#  define COLOR_DEFAULT  "\033[0m"
+#else
+#  define COLOR_RED      ""
+#  define COLOR_GREEN    ""
+#  define COLOR_YELLOW   ""
+#  define COLOR_BLUE     ""
+#  define COLOR_MAGENTA  ""
+#  define COLOR_DEFAULT  ""
