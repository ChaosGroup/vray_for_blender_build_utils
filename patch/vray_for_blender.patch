+        fprintf(gfile, "\n}\n\n");
+
+
+        // Restore "Display percentage" setting
+        pset->disp      = display_percentage;
+        pset->child_nbr = display_percentage_child;
+
+        if(!free_edit) {
+            psys->recalc |= PSYS_RECALC;
