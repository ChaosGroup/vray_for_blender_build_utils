+
+void StripString(char *str)
+{
+	int nChars = strlen(str);
+	int i      = 0;
+
+	for(i = 0; i < nChars; i++) {
+		if(str[i]) {
+			if(str[i] == '+')
+				str[i] = 'p';
+			else if(str[i] == '-')
+				str[i] = 'm';
+			else if(!((str[i] >= 'A' && str[i] <= 'Z') || (str[i] >= 'a' && str[i] <= 'z') || (str[i] >= '0' && str[i] <= '9')))
+				str[i] = '_';
+		}
+	}
+}
